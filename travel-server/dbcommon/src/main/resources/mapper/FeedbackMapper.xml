  <update id="updateFeedBack" parameterType="hashmap" >
    update t_feedback
    <set >
      <if test="user_id != null" >
        user_id = #{user_id},
      </if>
      <if test="content != null" >
        content = #{content},
      </if>
      <if test="status != null" >
        status = #{status},
      </if>
      <if test="description != null" >
        description = #{description},
      </if>
      <if test="phone_number != null" >
        phone_number = #{phone_number},
